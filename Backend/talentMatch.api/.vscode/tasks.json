{
	"version": "2.0.0",
	"tasks": [
	  {
		"label": "build",
		"type": "shell",
		"command": "dotnet build",
		"group": {
		  "kind": "build",
		  "isDefault": true
		},
		"problemMatcher": "$msCompile"
	  },
	  {
		"label": "run",
		"type": "shell",
		"command": "dotnet run",
		"group": {
		  "kind": "test",
		  "isDefault": true
		},
		"dependsOn": ["build"],
		"problemMatcher": "$msCompile"
	  }
	]
  }
  